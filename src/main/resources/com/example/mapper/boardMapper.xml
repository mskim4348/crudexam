<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.boardMapper">
	<select id="list" resultType="com.example.dto.board">
		select * from board;
	</select>
	
	<insert id="insert" parameterType="com.example.dto.board">
			insert into board (bno,title,content,writer,viewcnt) values(nextval('boardSeq'),#{title},#{content},#{write},#{viewcnt});
	</insert>
</mapper>
